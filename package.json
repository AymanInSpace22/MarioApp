{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "main": "main.cjs",
  "scripts": {
    "start": "vite",
    "electron": "electron .",
    "build": "vite build",
    "dev": "concurrently \"npm:start\" \"npm run electron\"",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "Mario-APP-Dev",
    "win": {
      "target": "nsis"
    },
    "productName": "Mario-APP-Dev",
    "icon": "public/icon2.ico"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.0.0",
    "concurrently": "^8.2.0",
    "electron": "25.1.1",
    "electron-builder": "^24.4.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "express": "^4.18.2",
    "vite": "^4.3.9"
  }
}
